export const t = (lang: Language, key: string, options?: Record<string, string | number>): string => {
    const langDict = translations[lang] || translations['uz-Latn'];
    let text = get(langDict, key);

    if (text === undefined) {
        console.warn(`Translation key not found: ${key}`);
        return key;
    }

    if (options) {
        Object.keys(options).forEach((k) => {
            text = text!.replace(`{${k}}`, String(options[k]));
        });
    }

    return text;
};